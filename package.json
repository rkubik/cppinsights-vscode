{
	"name": "cppinsights",
	"displayName": "cppinsights",
	"description": "See your source code with the eyes of a compiler",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cppinsights.selection",
		"onCommand:cppinsights.file"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "cppinsights",
				"properties": {
					"cppinsights.insightsPath": {
						"type": "string",
						"default": "",
						"description": "Absolute path to insights binary",
						"scope": "resource"
					}
				}
			}
		],
		"commands": [
			{
				"command": "cppinsights.selection",
				"title": "cppinsights selection"
			},
			{
				"command": "cppinsights.file",
				"title": "cppinsights file"
			}
		],
		"keybindings": [
			{
				"command": "cppinsights.selection",
				"key": "ctrl+shift+l",
				"when": "editorTextFocus"
			},
			{
				"command": "cppinsights.file",
				"key": "ctrl+shift+j",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0",
		"tmp": "^0.1.0"
	}
}
